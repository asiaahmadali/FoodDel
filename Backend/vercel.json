{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
   "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
   "functions": {
    "api/*.js": {
      "maxDuration": 30
    }
  },
}
